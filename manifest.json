{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/jquery-1.7.2.min.js", "js/application.js" ],
      "matches": [ "\u003Call_urls\u003E" ],
      "run_at": "document_end"
   } ],
   "description": "Curator for Techive",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 1,
   "name": "Curator for Techive.org",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "version": "1.0"
}
